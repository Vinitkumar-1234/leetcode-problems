class BSTIterator:
    res=deque()
    def __init__(self, root: Optional[TreeNode]):
        def inorder(root):
            if root:
                inorder(root.left)
                self.res.append(root.val)
                inorder(root.right)
        inorder(root)        

    def next(self) -> int:
        return self.res.popleft()

    def hasNext(self) -> bool:
        return False if  not self.res else True  
